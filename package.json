{
  "name": "print-desktop",
  "version": "1.0.0",
  "main": "build/electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode production",
    "electron:dev": "vite --config vite.config.electron.ts",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "preview": "vite preview",
    "electron:build:win": "vite build --config vite.config.electron.ts && electron-builder --win",
    "electron:build:mac": "vite build --config vite.config.electron.ts && electron-builder --arm64",
    "prepare": "husky install",
    "test": "electron-builder --dir"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.20.1",
    "axios": "^1.3.2",
    "chalk": "^5.3.0",
    "electron-reloader": "^1.2.3",
    "electron-store": "^9.0.0",
    "electron-updater": "^6.2.1",
    "fs-extra": "^11.3.0",
    "hoslink-xxx": "^1.1.1",
    "multispinner": "^0.2.1",
    "node-fetch": "^3.3.2",
    "pdf-to-printer": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.8.1",
    "universalify": "^2.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,tsx,ts}": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/node": "18.11.18",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "commitizen": "4.3.0",
    "commitlint": "17.6.5",
    "commitlint-config-cz": "0.13.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "6.3.0",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.63.2",
    "typescript": "*",
    "unplugin-auto-import": "^0.16.7",
    "vite": "^4.3.9",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-require-transform": "^1.0.5",
    "vite-plugin-webpackchunkname": "^0.2.4"
  },
  "build": {
    "appId": "com.my-website.print-desktop",
    "productName": "print-desktop",
    "copyright": "Copyright Â© 2025",
    "asar": true,
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/icon/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon/icon.ico"
    },
    "nsis": {
      "language": 2052,
      "artifactName": "${productName}_Windows_Setup_${version}.${ext}",
      "oneClick": false,
      "createDesktopShortcut": true,
      "allowElevation": true,
      "createStartMenuShortcut": true,
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "build_exe/${version}"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}